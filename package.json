{
  "name": "doj-react-adminlte",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.6.1",
    "react-datetime": "^2.14.0",
    "react-popper": "^1.0.0-beta.6",
    "react-resize-aware": "^2.7.0",
    "react-select": "^1.2.1",
    "react-smooth-collapse": "^1.5.0",
    "react-transition-group": "^2.3.1"
  },
  "scripts": {
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "start:docs": "react-scripts start",
    "gen:docs": "node scripts/generate-component-data.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "build:docs": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm run build:commonjs && npm run build:css",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore spec.js",
    "eject": "react-scripts eject"
  },
  "peerDependencies": {
    "moment": "^2.22.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@vallarj/react-scripts": "1.0.0",
    "babel-cli": "^6.26.0",
    "chokidar": "^2.0.3",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.6",
    "moment": "^2.22.1",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.4",
    "rimraf": "^2.6.2"
  }
}
